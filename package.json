{
  "name": "webpack-typescript-starter",
  "version": "0.0.1",
  "description": "A simple Webpack 5 starter with TypeScript transpilation",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --port 9000",
    "build": "webpack --config webpack.config.js --mode development",
    "build.prod": "webpack --config webpack.config.js --mode production",
    "build.esnext": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts\" --source-maps inline",
    "build.all": "npm run build.prod && npm run build.esnext",
    "test": "npx jest"
  },
  "keywords": [
    "webpack",
    "webpack5",
    "typescript"
  ],
  "author": "xcchcaptain@126.com",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-typescript": "^7.15.0",
    "@types/node": "7.0.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^27.2.0",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^6.7.0",
    "source-map-loader": "0.2.3",
    "style-loader": "^3.2.1",
    "css-loader": "^6.2.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.3",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^4.0.0-beta.0"
  }
}
